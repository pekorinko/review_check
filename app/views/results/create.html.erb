<header>
  <%= render partial: 'results/form' %>
</header>
<h2 class="mt-3 mb-3 mx-5">チェック結果</h2>
<div class="mt-3 mb-3 mx-5">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>店舗名：<%= @place.place_name %></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>住所：<%= @place.address %></td>
      <tr>
      <tr>
        <td>平均投稿数：<%= @result.count_ave %></td>
      <tr>
      <tr>
        <td>平均星評価：<%= @result.star_ave %></td>
      <tr>
      <tr>
        <td>平均文字数：<%= @result.text_ave %></td>
      <tr>
      <tr>
        <td>厳選された星評価：<%= @result.credible_star_ave %></td>
      <tr>
    </tbody>
  </table>
  <%= link_to "戻る",new_result_path %>
</div>
