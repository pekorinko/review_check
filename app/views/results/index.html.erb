    <header>
      <div class="alert alert-info text-center mb-0 rounded-0 alert-dismissible fade show">
          <% if flash.notice %>
            <%= flash.notice %>
          <% end %>
          <button class="close" data-dismiss="alert">
            &times;
          </button>
      </div>
        <%= render partial: 'results/form' %>
    </header>
    <main>
        <h2 class="my-3 mx-5"><%= current_user.name %>さんの閲覧履歴</h2>
        <div class="mt-3 mb-3 mx-5">
        <table class="table table-hover">
        <% @search_histories.each do |search_history| %>
            <!--<div class="mt-3 mb-3 mx-5">-->
                <thead>
                  <tr><th>店舗名：<%= search_history.place_name %></th></tr>
                </thead>
                <tbody>
                  <tr><td>平均星評価：<%= search_history.star_ave %></td></tr>
                  <tr><td>厳選された星評価：<%= search_history.credible_star_ave %></td></tr>
                  <tr><td><%= link_to "詳細へ", result_path(search_history.result_id) %></td></tr>

                <!--
                <p>店舗名：<%= search_history.place_name %></p>
                <p>平均星評価：<%= search_history.star_ave %></p>
                <p>厳選された星評価：<%= search_history.credible_star_ave %></p>
                <%= link_to "詳細へ", result_path(search_history.result_id) %>
                -->
            </div>
        <% end %>
        </tbody>
        </table>
    </main>
 </body>
</html>
